{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.10.1",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./dist/main/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js --env.production --progress --colors",
    "lint:ts": "tslint -p ./tsconfig.json -t stylish",
    "lint:sass": "sass-lint --config .sass-lint.yml -v",
    "dev": "webpack --config webpack.dev.js --progress --colors --watch",
    "museeks": "./node_modules/.bin/electron .",
    "museeks:debug": "./node_modules/.bin/electron . --enable-logging --devtools",
    "package:lmw": "build -lmw --ia32 --x64",
    "package:l": "build -l --ia32 --x64",
    "package:w": "build -w --ia32 --x64",
    "package:m": "build -m"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "bootstrap-css-only": "3.3.7",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "globby": "8.0.1",
    "history": "4.7.2",
    "level-js": "3.0.0",
    "linvodb3": "3.26.0",
    "lodash-es": "4.17.11",
    "music-metadata": "3.2.3",
    "queue": "4.5.0",
    "react": "16.6.3",
    "react-bootstrap": "0.32.4",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.6.3",
    "react-fontawesome": "1.6.1",
    "react-keybinding-component": "1.0.0",
    "react-rangeslider": "2.2.0",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.3.1",
    "react-simple-input": "0.4.2",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "semver": "5.6.0",
    "svg-inline-react": "3.1.0",
    "teeny-conf": "4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.24",
    "@types/classnames": "2.2.6",
    "@types/globby": "8.0.0",
    "@types/history": "4.7.2",
    "@types/lodash-es": "4.17.1",
    "@types/react": "16.7.7",
    "@types/react-bootstrap": "0.32.14",
    "@types/react-custom-scrollbars": "4.0.5",
    "@types/react-dom": "16.0.10",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-redux": "6.0.10",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-logger": "3.0.6",
    "@types/semver": "5.5.0",
    "clean-terminal-webpack-plugin": "1.1.0",
    "css-loader": "1.0.1",
    "electron": "3.0.10",
    "electron-builder": "20.36.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "install": "0.12.2",
    "mini-css-extract-plugin": "0.4.5",
    "node-sass": "4.10.0",
    "redux-logger": "3.0.6",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "svg-inline-loader": "0.8.0",
    "terser-webpack-plugin": "1.1.0",
    "ts-loader": "5.3.0",
