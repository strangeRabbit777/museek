{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.11.5",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./dist/main/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "webpack --config webpack.prod.js --color",
    "dev": "webpack --config webpack.dev.js --color --watch",
    "museeks": "electron .",
    "museeks:debug": "electron . --enable-logging --devtools",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "test:formatting:fix": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "test:css": "stylelint \"src/**/*.css\"",
    "package:lmw": "electron-builder -lmw && npm run package:checksums",
    "package:lw": "electron-builder -lw",
    "package:l": "electron-builder -l",
    "package:w": "electron-builder -w",
    "package:m": "electron-builder -m",
    "package:checksums": "bash scripts/checksum.sh"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "bluebird": "3.7.2",
    "chardet": "1.3.0",
    "classnames": "2.3.1",
    "electron-devtools-installer": "^3.1.1",
    "font-awesome": "4.7.0",
    "globby": "11.0.2",
    "history": "4.10.1",
    "iconv-lite": "0.6.2",
    "level-js": "4.0.1",
    "linvodb3": "3.26.0",
    "lodash-es": "4.17.20",
    "m3ujs": "0.2.1",
    "mime-types": "2.1.28",
    "mpris-service": "2.1.0",
    "music-metadata": "7.7.0",
    "postcss-scss": "3.0.4",
    "ps-node": "0.1.6",
    "queue": "6.0.2",
    "rc-scrollbars": "^1.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-fontawesome": "1.7.1",
    "react-keybinding-component": "1.0.0",
    "react-onclickout": "2.0.8",
    "react-rangeslider": "2.2.0",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-simple-input": "0.4.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "2.3.0",
    "semver": "7.3.4",
    "svg-inline-react": "3.2.0",
    "teeny-conf": "4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.35",
    "@types/classnames": "2.2.11",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "4.7.8",
    "@types/lodash-es": "4.17.4",
    "@types/mime-types": "2.1.0",
    "@types/react": "17.0.11",
    "@types/react-custom-scrollbars": "4.0.7",
    "@types/react-dom": "17.0.8",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/redux-logger": "3.0.8",
    "@types/semver": "7.3.6",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "clean-terminal-webpack-plugin": "3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "5.2.6",
    "electron": "13.1.3",
    "electron-builder": "22.11.7",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "install": "0.13.0",
    "mini-css-extract-plugin": "1.6.0",
    "node-loader": "2.0.0",
    "normalize.css": "8.0.1",
    "postcss": "^8.3.5",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.1.0",
    "postcss-nested": "5.0.5",
    "prettier": "2.3.1",
    "prettier-eslint": "12.0.0",
    "redux-logger": "3.0.6",
    "source-map-loader": "3.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-standard": "22.0.0",
    "svg-inline-loader": "0.8.2",
    "terser-webpack-plugin": "5.1.3",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "webpack": "5.40.0",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.0-3"
  }
}
