{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.9.0",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./src/main-process/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeitIG/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KeitIG/museeks/issues"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js --env.production --progress --colors",
    "lint:js": "eslint . --config .eslintrc",
    "lint:sass": "sass-lint --config .sasslintrc -v -q",
    "dev": "webpack --config webpack.dev.js --progress --colors --watch",
    "museeks": "./node_modules/electron/cli.js .",
    "museeks:debug": "./node_modules/electron/cli.js . --enable-logging",
    "package:lmw": "build -lmw --ia32 --x64",
    "package:l": "build -l --ia32 --x64",
    "package:w": "build -w --ia32 --x64",
    "package:m": "build -m"
  },
  "build": {
    "appId": "io.museeks.app",
    "electronVersion": "1.8.3",
    "directories": {
      "buildResources": "./dist",
      "output": "./build"
    },
    "files": [
      "dist/**/*",
      "src/images/**/*",
      "src/main-process/**/*",
      "src/shared/**/*",
      "node_modules/teeny-conf/**/*"
    ],
    "mac": {
      "category": "public.app-category.music",
      "target": [
        "dmg"
      ],
      "icon": "src/images/logos/museeks.icns"
    },
    "linux": {
      "category": "AudioVideo",
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ],
      "executableName": "museeks"
    },
    "win": {
      "target": "nsis",
      "icon": "src/images/logos/museeks.ico"
    }
  },
  "dependencies": {
    "bootstrap-css-only": "3.3.7",
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "globby": "8.0.1",
    "history": "4.7.2",
    "keymirror": "0.1.1",
    "level-js": "2.2.4",
    "linvodb3": "3.25.1",
    "lodash.orderby": "4.6.0",
    "music-metadata": "0.9.12",
    "open-sans-fontface": "1.4.0",
    "prop-types": "15.6.1",
    "queue": "4.4.2",
    "react": "16.3.0",
    "react-bootstrap": "0.32.1",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.3.0",
    "react-fontawesome": "1.6.1",
    "react-keybinding-component": "0.5.1",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.2.2",
    "react-simple-input": "0.4.1",
    "react-svg-inline": "2.1.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "semver": "5.5.0",
    "teeny-conf": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-terminal-webpack-plugin": "1.0.1",
    "css-loader": "0.28.11",
    "electron": "1.8.4",
    "electron-builder": "20.8.1",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.1.0",
    "install": "0.11.0",
    "json-loader": "0.5.7",
    "node-sass": "4.8.3",
    "redux-logger": "3.0.6",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "svg-inline-loader": "0.8.0",
    "url-loader": "1.0.1",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.2"
  }
}
